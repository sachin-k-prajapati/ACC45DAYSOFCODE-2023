#include <stdio.h>
#include <math.h>
int main(){
  int n;
  printf("Enter the number to check its divisibility by 5 and 11\n");
  scanf("%d", &n);
  if(n%5==0 && n%11==0){
    printf("The number is divisible by 5 and 11");
  } else if(n%5==0 && n%11!=0){
    printf("The number is divisible by 5 but not by 11");
  } else if(n%5!=0 && n%11==0){
    printf("The number is divisible by 11 but not by 5");
  } else {
    printf("The number is not divisible by 5 and 11");
  }
  return 0;
}
