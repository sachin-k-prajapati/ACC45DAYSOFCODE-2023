#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int X, Y, Z;
        scanf("%d %d %d", &X, &Y, &Z);

        // Calculate the total amount Chef has
        int total_money = X * 5 + Y * 10;

        // Calculate the maximum number of chocolates Chef can buy
        int max_chocolates = total_money / Z;

        printf("%d\n", max_chocolates);
    }

    return 0;
}
