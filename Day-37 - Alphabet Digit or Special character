#include<stdio.h>

int main() {
  char ch;
  printf("\nEnter Any Character : ");
  scanf("%c", & ch);
  if (ch >= '0' && ch <= '9') {
    printf("\nEntered Character is DIGIT");
  } else if (ch >= 'A' && ch <= 'Z') {
    printf("\nEntered Character is ALPHABET");
  } else if (ch >= 'a' && ch <= 'z') {
    printf("\nEntered Character is ALPHABET");
  } else {
    printf("\nEntered Character is SPECIAL CHARACTER");
  }
  return 0;
}
